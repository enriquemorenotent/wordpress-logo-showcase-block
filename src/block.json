{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "emt/logo-showcase",
	"version": "0.1.0",
	"title": "EMT Logo Showcase",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false,
		"align": true
	},
	"textdomain": "emt-logo-showcase",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"attributes": {
		"tagline": {
			"type": "string",
			"default": "Lorem ipsum"
		},
		"itemsPerPage": {
			"type": "number",
			"default": 3
		},
		"images": {
			"type": "array",
			"default": []
		}
	}
}
